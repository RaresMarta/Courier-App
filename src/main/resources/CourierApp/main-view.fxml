<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17"
      fx:controller="CourierApp.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <HBox prefHeight="329.0" prefWidth="497.0">
            <children>
                <ListView fx:id="pListView" prefHeight="329.0" prefWidth="391.0" />
                <Button mnemonicParsing="false" onAction="#addPackage" prefHeight="51.0" prefWidth="107.0" text="Add Package" />
                <Button onAction="#addCourier" text="Add Courier" />
            </children>
        </HBox>
    </children>
</VBox>